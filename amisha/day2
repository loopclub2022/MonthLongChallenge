class Solution {
    public int[] sortArrayByParity(int[] nums) 
    {
        int i=0, temp;
        for(int j=0;j<nums.length;j++)
            if(nums[j] % 2 == 0)
        {
            temp=nums[i];
            nums[i++]=nums[j];
            nums[j]=temp;
              
        }
        return nums;
    }
}
